<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!--圆形进度条的自定义属性 自己可以扩展-->
    <declare-styleable name="HoloCircularProgressBar">
        <!--圆环宽度-->
        <attr name="stroke_width" format="dimension" />
        <!--当前进度-->
        <attr name="progress" format="float" />
        <!--刻度显示的角度-->
        <attr name="marker_progress" format="float" />
        <!--当前进度的颜色-->
        <attr name="progress_color" format="color" />
        <!--进度条背景颜色-->
        <attr name="progress_background_color" format="color" />
        <!--是否显示进度条头部的块-->
        <attr name="thumb_visible" format="boolean" />
        <!--是否显示刻度-->
        <attr name="marker_visible" format="boolean" />
        <attr name="android:gravity" />
    </declare-styleable>

    <attr name="circularProgressBarStyle" format="reference" />



    <!--tab切换时切换的控件的自定义属性-->
    <declare-styleable name="TitleIndicator">
        <attr name="titlePadding" format="dimension" />
        <!-- Left/right padding of not active view titles. -->
        <attr name="clipPadding" format="dimension" />
        <attr name="textColor" format="color|reference" />
        <attr name="textSizeNormal" format="dimension" />
        <attr name="textSizeSelected" format="dimension" />
        <attr name="footerLineHeight" format="dimension" />
        <attr name="footerColor" format="color" />
        <attr name="footerTriangleHeight" format="dimension" />
    </declare-styleable>


    <!--九宫格布局-->
    <declare-styleable name="GiftDetailGridLayout_Layout">
        <attr name="android:layout_gravity" />
        <attr name="layout_sticky" format="boolean" />
    </declare-styleable>
    <declare-styleable name="GiftDetailGridLayout">
        <attr name="android:rowCount" />
        <attr name="android:columnCount" />
        <attr name="rowWeight" format="string" />
        <attr name="columnWeight" format="string" />
    </declare-styleable>
</resources>